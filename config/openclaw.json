  "plugins": {
    "entries": {
      "slack": {
        "enabled": true
      },
      "twilio-sms": {
        "enabled": true
      },
      "voice-call": {
        "enabled": true,
        "config": {
          "enabled": true,
          "provider": "deepgram",
          "defaultAgentId": "voice-agent",
          "numbers": {
            "+16087057765": {
              "agentId": "voice-agent",
              "direction": "both"
            }
          },
          "fromNumber": "+16087057765",
          "deepgram": {
            "stt": {
              "model": "nova-3"
            },
            "tts": {
              "model": "aura-2-thalia-en"
            },
            "language": "en-US",
            "latency": {
              "fillerThresholdMs": 3000,
              "fillerPhrases": [
                "One moment please. ",
                "Let me look into that. ",
                "Sure, let me check. ",
                "Just a moment. "
              ],
              "dynamicFiller": true
            }
          },
          "skipSignatureVerification": true,
          "serve": {
            "port": 3334,
            "bind": "0.0.0.0",
            "path": "/voice/webhook"
          },
          "timezone": "America/New_York",
          "inboundPolicy": "open"
        }
      }
    }
