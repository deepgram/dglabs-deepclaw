# Docker Compose environment variables for OpenClaw
# Copy to .env and fill in your values

# --- Docker image ---
OPENCLAW_IMAGE=openclaw:local

# --- Gateway ---
OPENCLAW_GATEWAY_TOKEN=
OPENCLAW_GATEWAY_PORT=18789
OPENCLAW_GATEWAY_BIND=lan
OPENCLAW_BRIDGE_PORT=18790
OPENCLAW_VOICE_PORT=8000

# --- Paths ---
OPENCLAW_CONFIG_DIR=
OPENCLAW_WORKSPACE_DIR=

# --- API Keys ---
DEEPGRAM_API_KEY=
OPENAI_API_KEY=

# --- Twilio (WhatsApp) ---
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=

# --- Claude ---
CLAUDE_AI_SESSION_KEY=
CLAUDE_WEB_SESSION_KEY=
CLAUDE_WEB_COOKIE=

# --- Public URL (for webhooks / tunnels) ---
PUBLIC_URL=
